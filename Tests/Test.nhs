func (int) Sum(var:int a, var:int b){
	var summed = a + b;
	return summed;
}

func (int) Fib(var:int N) {
	var i = 1;
	var curr = 1;
	var prev = 0;
	var temp = 0;

	while i<N {
		temp = curr;
		curr = Sum(curr, prev);
		prev = temp;

		i = i + 1;
	}

	return curr;
}

var result = Fib(40);
print("Result: ", result);

