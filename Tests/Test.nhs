func (int) Sum(var:int a, var:int b){
	var summed = a + b;
	return summed;
}

func (int, string) Fib(var:int N) {
	var i = 1;
	var curr = 1;
	var prev = 0;
	var temp = 0;

	if N < 0{
		return (-1, "Error: Negative input");
	}


	while i<N {
		temp = curr;
		curr = Sum(curr, prev);
		prev = temp;

		i = i + 1;
	}

	return (curr, "Complete!");
}

var:float x = Fib(100000000);
print(x);